{
  "name": "todo-list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-host": "ng serve --disable-host-check",
    "start:ssl": "ng serve --disable-host-check --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /projects/listet/",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run todo-list:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run todo-list:server:production",
    "prerender": "ng run todo-list:prerender",
    "postinstall": "ngcc",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.6",
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/forms": "~12.2.6",
    "@angular/platform-browser": "~12.2.6",
    "@angular/platform-browser-dynamic": "~12.2.6",
    "@angular/platform-server": "~12.2.6",
    "@angular/router": "~12.2.6",
    "@nguniversal/express-engine": "~12.1.0",
    "angular-feather": "^6.1.0",
    "bufferutil": "^4.0.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.9.1",
    "express": "^4.15.2",
    "masonry-layout": "^4.2.2",
    "rxjs": "~6.6.6",
    "sitemap-generator": "^8.5.1",
    "snyk": "^1.681.0",
    "socket.io-client": "^3.0.5",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.4",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/language-service": "~12.1.1",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "socket.io": "^3.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}