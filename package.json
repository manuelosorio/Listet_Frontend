{
  "name": "listet_frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-host": "ng serve --host 127.0.0.1 --public-host localhost",
    "start:ssl": "ng serve --host 127.0.0.1 --public-host listet.local --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /projects/listet/",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run listet:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run listet:server:production",
    "prerender": "ng run listet:prerender",
    "postinstall": "ngcc",
    "snyk:protect": "snyk-protect",
    "snyk:test": "snyk test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/platform-server": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@angular/ssr": "^17.3.9",
    "@snyk/protect": "^1.1105.0",
    "angular-feather": "^6.5.0",
    "bufferutil": "^4.0.6",
    "core-js": "^3.27.2",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "rxjs": "~7.5.0",
    "socket.io-client": "^3.0.5",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.9",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^17.3.9",
    "@angular-eslint/builder": "^17.5.2",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "^17.3.9",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/express": "^4.17.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "browser-sync": "^3.0.0",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
