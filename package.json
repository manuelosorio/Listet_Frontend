{
  "name": "listet_frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-host": "ng serve --disable-host-check",
    "start:ssl": "ng serve --disable-host-check --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /projects/listet/",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run listet:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run listet:server:production",
    "prerender": "ng run listet:prerender",
    "postinstall": "ngcc",
    "snyk:protect": "snyk-protect",
    "snyk:test": "snyk test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/platform-server": "^15.1.5",
    "@angular/router": "^15.1.5",
    "@nguniversal/express-engine": "^15.2.0",
    "@snyk/protect": "^1.1105.0",
    "angular-feather": "^6.5.0",
    "bufferutil": "^4.0.6",
    "core-js": "^3.27.2",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "masonry-layout": "^4.2.2",
    "rxjs": "~6.6.6",
    "socket.io-client": "^3.0.5",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.9",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@angular/language-service": "^15.1.5",
    "@nguniversal/builders": "^15.1.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/express": "^4.17.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "socket.io": "^3.1.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.4"
  }
