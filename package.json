{
  "name": "listet_frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-host": "ng serve --disable-host-check",
    "start:ssl": "ng serve --disable-host-check --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /projects/listet/",
    "test": "ng test --verbose",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run listet:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run listet:server:production",
    "prerender": "ng run listet:prerender",
    "postinstall": "ngcc",
    "snyk:protect": "snyk-protect",
    "snyk:test": "snyk test"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^16.0.1",
    "@angular/animations": "^16.2.10",
    "@angular/common": "^16.2.10",
    "@angular/compiler": "^16.2.10",
    "@angular/core": "^16.2.10",
    "@angular/forms": "^16.2.10",
    "@angular/platform-browser": "^16.2.10",
    "@angular/platform-browser-dynamic": "^16.2.10",
    "@angular/platform-server": "^16.2.10",
    "@angular/router": "^16.2.10",
    "@nguniversal/express-engine": "^16.2.0",
    "@snyk/protect": "^1.1105.0",
    "@types/jest": "^29.5.6",
    "angular-feather": "^6.5.0",
    "bufferutil": "^4.0.6",
    "core-js": "^3.27.2",
    "domino": "^2.1.6",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jest": "^29.7.0",
    "masonry-layout": "^4.2.2",
    "rxjs": "~6.6.6",
    "socket.io-client": "^3.0.5",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.9",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^16.2.7",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.7",
    "@angular/compiler-cli": "^16.2.10",
    "@angular/language-service": "^16.2.10",
    "@nguniversal/builders": "^16.2.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/express": "^4.17.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.39.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
