{
  "name": "listet_frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:no-host": "ng serve --disable-host-check",
    "start:ssl": "ng serve --disable-host-check --ssl true --ssl-cert ./ssl/server.crt --ssl-key ./ssl/server.key",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /projects/listet/",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run listet:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run listet:server:production",
    "prerender": "ng run listet:prerender",
    "postinstall": "ngcc",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.2",
    "@angular/common": "^14.0.2",
    "@angular/compiler": "^14.0.2",
    "@angular/core": "^14.0.2",
    "@angular/forms": "^14.0.2",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/platform-server": "^14.0.2",
    "@angular/router": "^14.0.2",
    "@nguniversal/express-engine": "^14.0.1",
    "angular-feather": "^6.3.0",
    "bufferutil": "^4.0.6",
    "core-js": "^3.23.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "masonry-layout": "^4.2.2",
    "rxjs": "~6.6.6",
    "snyk": "^1.883.0",
    "socket.io-client": "^3.0.5",
    "tslib": "^2.3.1",
    "utf-8-validate": "^5.0.9",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0-beta.0",
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular-eslint/builder": "^14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin": "^14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin-template": "^14.0.0-alpha.3",
    "@angular-eslint/schematics": "14.0.0-alpha.3",
    "@angular-eslint/template-parser": "14.0.0-alpha.3",
    "@angular/cli": "^14.0.2",
    "@angular/compiler-cli": "^14.0.2",
    "@angular/language-service": "^14.0.2",
    "@nguniversal/builders": "^14.0.1",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/express": "^4.17.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "socket.io": "^3.1.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.3"
  }
}
